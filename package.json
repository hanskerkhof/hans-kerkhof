{
  "name": "hanskerkhof",
  "version": "3.3.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --coverage --maxWorkers=4",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run hanskerkhof:serve-ssr",
    "serve:ssr": "node dist/hanskerkhof/server/main.js",
    "build:ssr": "ng build --prod && ng run hanskerkhof:server:production",
    "prerender": "ng run hanskerkhof:prerender --routesFile=routes.txt",
    "deploy-aws-s3-prev": "npm run prerender && aws s3 sync dist/hanskerkhof s3://hanskerkhof --delete --region eu-west-1 --acl public-read && aws s3 cp dist/hanskerkhof/index.html s3://hanskerkhof/index.html --metadata Cache-Control=max-age=0 --cache-control no-cache --expires 0 --acl public-read",
    "deploy-aws-s3-old": "npm --no-git-tag-version version patch && aws s3 sync src/assets/i18n s3://hanskerkhof/api/i18n --region eu-west-1 --acl public-read && aws s3 cp src/assets/content/resume.json s3://hanskerkhof/api/resume.json --region eu-west-1 --acl public-read && npm run prerender && aws s3 sync dist/hanskerkhof/browser s3://hanskerkhof --region eu-west-1 --acl public-read && aws s3 cp dist/hanskerkhof/browser/index.html s3://hanskerkhof/index.html --metadata Cache-Control=max-age=0 --cache-control no-cache --expires 0 --acl public-read",
    "deploy-aws-s3": "scripts/deploy-aws-s3.sh",
    "deploy-gh-pages": "ng deploy --no-silent --name='CI/CD' --email='info@netmatic.nl' --message='deploy-gh-pages [skip ci]\n\nSkip this commit on CircleCi '"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~11.2.9",
    "@angular/common": "~11.2.9",
    "@angular/compiler": "~11.2.9",
    "@angular/core": "~11.2.9",
    "@angular/forms": "~11.2.9",
    "@angular/platform-browser": "~11.2.9",
    "@angular/platform-browser-dynamic": "~11.2.9",
    "@angular/platform-server": "~11.2.9",
    "@angular/router": "~11.2.9",
    "@nguniversal/express-engine": "^10.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@types/jest": "^26.0.3",
    "bootstrap": "^4.5.0",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "generate": "^0.14.0",
    "jest": "^26.1.0",
    "jest-preset-angular": "^8.2.1",
    "moment": "^2.27.0",
    "ngx-google-analytics": "^9.0.0",
    "ngx-meta-tags": "^0.2.1",
    "ngx-moment": "^4.0.1",
    "resume-schema": "^0.1.3",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.8",
    "@angular/cli": "~11.2.8",
    "@angular/compiler-cli": "~11.2.9",
    "@nguniversal/builders": "^10.0.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.2.4",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/cypress/",
      "src/test.ts"
    ]
  }
}
